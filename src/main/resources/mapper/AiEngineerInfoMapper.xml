    <select id="selectListByMap" parameterType="java.lang.String" resultMap="BaseResultMap_ai">
        select
        <include refid="Base_Column_List_ai"/>
        from aiengineerinfo
        where 1 = 1
        <if test="name != null">
            and Jobname like '%${name}%'
        </if>
        <if test="numberBegin != null">
            and jobnum &gt;= #{numberBegin}
        </if>
        <if test="numberEnd != null">
            and jobnum &lt;= #{numberEnd}
        </if>
        and orgid = #{orgId}
        <if test="startrow != null and pagesize != null">
            limit #{startrow} , #{pagesize}
